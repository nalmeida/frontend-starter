{
	"name": "frontend-starter",
	"version": "1.0.0",
	"description": "Front-end Starter using Gulp + Tailwind 1.1.2 + Browsersync + JS + Image",
	"main": "gulpfile.js",
	"scripts": {
		"dev": "./node_modules/.bin/gulp",
		"prod": "./node_modules/.bin/gulp build"
	},
	"options": {
		"config": {
			"tailwindjs": "./tailwind.config.js",
			"cacheBust": "false"
		},
		"paths": {
			"dev": {
				"folder": "./dev",
				"url": "//localhost:1234",
				"imgUrl": "//localhost:1234"
			},
			"prod": {
				"folder": "./prod",
				"url": "//www.yoursite.com",
				"imgUrl": "//www.yoursite.com"
			}
		}
	},
	"keywords": [],
	"author": "Nicholas Almeida",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^9.6.4",
		"browser-sync": "^2.26.7",
		"del": "^3.0.0",
		"gulp": "^4.0.2",
		"gulp-clean-css": "^4.2.0",
		"gulp-concat": "^2.6.1",
		"gulp-file-include": "^2.1.0",
		"gulp-imagemin": "^5.0.3",
		"gulp-md5-assets": "^1.0.1",
		"gulp-postcss": "^8.0.0",
		"gulp-purgecss": "^1.2.0",
		"gulp-rename": "^1.4.0",
		"gulp-sass": "^4.0.2",
		"gulp-sitemap": "^7.5.0",
		"gulp-uglify": "^3.0.2",
		"tailwindcss": "^1.1.2"
	},
	"dependencies": {}
}
